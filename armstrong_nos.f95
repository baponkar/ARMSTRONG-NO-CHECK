PROGRAM ARMSTRONG_TEST
   IMPLICIT NONE
   INTEGER::DIG,I,TEMP,K=0,DIG_POWER=0,N,CHECK
    PRINT*,"ENTER A NUMBER TO CHECK"
     READ*,N
    TEMP=N

   DO
    IF(TEMP==0)THEN
     EXIT
    ELSE
     TEMP=TEMP/10
     K=K+1
    ENDIF
   END DO

   TEMP=N
   
   DO I=1,K
    
    IF(TEMP==0)THEN
     EXIT
    ELSE
    DIG=MOD(TEMP,10)
    TEMP=TEMP/10
    DIG_POWER=DIG_POWER+DIG**K
    ENDIF
   END DO

   IF(DIG_POWER == N)THEN
    CHECK=1
    PRINT*,"THIS IS A ARMSTRONG NO"
   ELSE
     CHECK=0
     PRINT*,"THIS IS NOT A ARMSTRONG NO"
   ENDIF
 
 END PROGRAM ARMSTRONG_TEST

    
    
     
